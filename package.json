{
    "name": "rn-bitcoin-icons",
    "description": "React Native Icons from the Bitcoin UI Kit",
    "version": "1.0.2",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "scripts": {
        "start": "yarn build:es --watch",
        "build:es": "BABEL_ENV=build babel src --root-mode upward --out-dir dist --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments",
        "build:declaration": "tsc --project tsconfig.json",
        "build:declaration:watch": "tsc --project tsconfig.json --watch"
    },
    "dependencies": {
        "@emotion/native": "^11.0.0",
        "@emotion/react": "^11.4.1",
        "react-native-svg": "^12.3.0"
    },
    "repository": "https://github.com/EttaWallet/rn-bitcoin-icons",
    "bugs": {
        "url": "https://github.com/EttaWallet/rn-bitcoin-icons/issues"
    },
    "homepage": "https://github.com/EttaWallet/rn-bitcoin-icons",
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.4",
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@svgr/cli": "^6.3.1",
        "react-native-svg-transformer": "^1.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "9ed7b92627dfbc016f05b55bf8ef3f24a14f7603"
}